<%- include('partials/header') -%>
<body>
<%- include('partials/navbar') -%>
<div class="container">
<h1>Questions</h1>
<hr>
<a href="/ask" class="btn btn-sky">Make a question</a>
    <hr>
<br>
<% questions.forEach(question =>{%>
<br>
    <div class="card">
        <div class="card-body">
            <h3><%= question.title %></h3>
        </div>
        <div class="card-footer">
           <a href="/question/<%= question.id %>"><button class="btn btn-sky">Replay</button></a>
        </div>
    </div>
<%})%>
</div>
</body>
<%- include('partials/footer') -%>
</html>